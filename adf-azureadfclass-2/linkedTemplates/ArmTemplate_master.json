{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-azureadfclass-2"
		},
		"lsadlsgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lsadlsgen2'"
		},
		"lsblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsblob'"
		},
		"lsadlsgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://saadlsclass2.dfs.core.windows.net/"
		},
		"lshttps_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().BaseURL}"
		},
		"httpWiki_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.kaggle.com/datasets/ahbab911/top-250-korean-dramas-kdrama-dataset"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-azureadfclass-2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lsadlsgen2_accountKey": {
						"value": "[parameters('lsadlsgen2_accountKey')]"
					},
					"lsblob_connectionString": {
						"value": "[parameters('lsblob_connectionString')]"
					},
					"lsadlsgen2_properties_typeProperties_url": {
						"value": "[parameters('lsadlsgen2_properties_typeProperties_url')]"
					},
					"lshttps_properties_typeProperties_url": {
						"value": "[parameters('lshttps_properties_typeProperties_url')]"
					},
					"httpWiki_properties_typeProperties_url": {
						"value": "[parameters('httpWiki_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}